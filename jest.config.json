{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
        "**/*.(t|j)s",
        "!**/*module.ts",
        "!**/enum/*",
        "!**/interface/*",
        "!**/index.ts"
    ],
    "roots": [
        "<rootDir>/src/"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
}